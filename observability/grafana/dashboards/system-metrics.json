{
  "title": "System Metrics",
  "uid": "system-metrics",
  "version": 1,
  "refresh": "30s",
  "timezone": "browser",
  "schemaVersion": 36,
  "tags": ["medicaments-api", "system"],
  "style": "dark",
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "links": [],
  "panels": [
    {
      "id": 1,
      "datasource": "Prometheus",
      "targets": [{"expr": "rate(process_cpu_seconds_total{job=\"system_scrapper\"}[5m]) * 100"}],
      "title": "CPU Usage",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "datasource": "Prometheus",
      "targets": [{"expr": "node_memory_MemAvailable_bytes / 1024 / 1024"}, {"expr": "(node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes) / 1024 / 1024"}],
      "title": "Memory Usage",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
    },
    {
      "id": 3,
      "datasource": "Prometheus",
      "targets": [{"expr": "go_goroutines"}],
      "title": "Goroutines",
      "type": "graph",
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 8}
    },
    {
      "id": 4,
      "datasource": "Prometheus",
      "targets": [{"expr": "process_open_fds"}, {"expr": "process_max_fds"}],
      "title": "Open File Descriptors",
      "type": "graph",
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 8}
    },
    {
      "id": 5,
      "datasource": "Prometheus",
      "targets": [{"expr": "rate(node_network_receive_bytes_total[5m])"}, {"expr": "rate(node_network_transmit_bytes_total[5m])"}],
      "title": "Network I/O",
      "type": "graph",
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 8}
    },
    {
      "id": 6,
      "datasource": "Prometheus",
      "targets": [{"expr": "rate(node_disk_read_bytes_total[5m])"}, {"expr": "rate(node_disk_written_bytes_total[5m])"}],
      "title": "Disk I/O",
      "type": "graph",
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 8}
    },
    {
      "id": 7,
      "datasource": "Prometheus",
      "targets": [{"expr": "go_memstats_heap_inuse_bytes / 1024 / 1024"}, {"expr": "go_memstats_stack_inuse_bytes / 1024 / 1024"}],
      "title": "Memory Allocation",
      "type": "graph",
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 16}
    },
    {
      "id": 8,
      "datasource": "Prometheus",
      "targets": [{"expr": "rate(go_gc_duration_seconds_sum[5m]) / rate(go_gc_duration_seconds_count[5m])"}],
      "title": "GC Duration",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
    }
  ],
  "time": {"from": "now-6h", "to": "now"},
  "templating": {"list": []},
  "annotations": {"list": []}
}
